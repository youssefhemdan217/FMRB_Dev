{
  "name": "@fmrb/backend",
  "version": "1.0.0",
  "description": "FMRB Backend API - Node.js + TypeScript + Express",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "build:prod": "npm run build && npm run setup:prod",
    "setup:prod": "DB_ENVIRONMENT=production node dist/setup-database.js",
    "setup:local": "DB_ENVIRONMENT=local node dist/setup-database.js",
    "deploy": "npm run build && npm run setup:prod",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}