    {
      "name": "üîê Authentication",
      "item": [
        {
          "name": "Register User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', response.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', response.refreshToken);",
                  "    pm.collectionVariables.set('userId', response.user.id);",
                  "    console.log('‚úÖ User registered! Token saved automatically.');",
                  "    console.log('User ID:', response.user.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"demo@example.com\",\n  \"password\": \"password123\",\n  \"name\": \"Demo User\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            },
            "description": "Register new user. Token automatically saved to collection variables."
          },
          "response": [
            {
              "code": 201,
              "name": "Created",
              "body": "{\n  \"user\": {\n    \"id\": \"3\",\n    \"email\": \"approver@example.com\",\n    \"name\": \"Approver One\",\n    \"role\": \"user\"\n  },\n  \"accessToken\": \"<jwt>\",\n  \"refreshToken\": \"<jwt>\"\n}"
            }
          ]
        },
        {
          "name": "Login User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', response.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', response.refreshToken);",
                  "    pm.collectionVariables.set('userId', response.user.id);",
                  "    console.log('‚úÖ Login successful! Token saved.');",
                  "    console.log('User:', response.user.name, '(' + response.user.role + ')');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"demo@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            },
            "description": "Login with credentials. Token automatically saved."
          },
          "response": [
            {
              "code": 200,
              "name": "OK",
              "body": "{\n  \"user\": {\n    \"id\": \"1\",\n    \"email\": \"Youssef@admin.com\",\n    \"name\": \"Youssef\",\n    \"role\": \"admin\"\n  },\n  \"accessToken\": \"<jwt>\",\n  \"refreshToken\": \"<jwt>\"\n}"
            }
          ]
        },
        {
          "name": "Logout User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    pm.collectionVariables.set('accessToken', '');",
                  "    pm.collectionVariables.set('refreshToken', '');",
                  "    console.log('‚úÖ Logged out successfully. Tokens cleared.');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/auth/logout",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "logout"]
            },
            "description": "Logout user. Requires authentication. Tokens will be cleared."
          },
          "response": []
        },
        {
          "name": "Update User Role (Make Admin/Approval)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    console.log('‚úÖ User role updated!');",
                  "    console.log('User:', response.user.name);",
                  "    console.log('New Role:', response.user.role);",
                  "    console.log('‚ö†Ô∏è Remember: Logout and login again to get new token with admin role!');",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"role\": \"admin\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/users/{{userId}}/role",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "users", "{{userId}}", "role"]
            },
            "description": "Update user role to admin or approval. Uses {{userId}} from login/register. IMPORTANT: Logout and login again after this!"
          },
          "response": [
            {
              "code": 200,
              "name": "OK",
              "body": "{\n  \"message\": \"Logged out successfully\",\n  \"timestamp\": \"2025-10-20T10:49:33.000Z\"\n}"
            }
          ]
        }
      ],
      "description": "User registration and login. Tokens are auto-saved after login!"
    },
